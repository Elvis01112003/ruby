<div id="<%= dom_id cuser %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= cuser.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Password:</strong>
    <%= cuser.password %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= cuser.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Mobile:</strong>
    <%= cuser.mobile %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Gender:</strong>
    <%= cuser.gender %>
  </div>
</div>
